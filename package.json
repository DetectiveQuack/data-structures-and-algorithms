{
  "name": "data-structures-algorithms",
  "version": "1.0.0",
  "description": "Data structures and algorithm examples",
  "main": "index.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./index",
    "test": "nyc mocha -r ts-node/register -r tsconfig-paths/register ./src/**/__tests__/**/*.spec.ts",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "coverage",
      "index.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "keywords": [
    "data",
    "structures",
    "algorithms"
  ],
  "author": "Saffaanh Soobratty",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.9.4",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.0.1",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "humanize-duration-ts": "^2.0.0",
    "performance-now": "^2.1.0",
    "winston": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DetectiveQuack/data-structures-and-algorithms.git"
  }
}
